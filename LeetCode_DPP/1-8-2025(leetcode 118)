class Solution {
public:
    vector<vector<int>> generate(int numRows) {
        vector<vector<int>> sol;
        for(int i=0;i<numRows;i++){
            vector<int> v(i+1,1);
            if(i>1){
                vector<int> vprev=sol[i-1];
                for(int j=1;j<v.size()-1;j++){
                    v[j]=vprev[j-1]+vprev[j];
                }
            }
            sol.push_back(v);
        }
        return sol;
    }
};
